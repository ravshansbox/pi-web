{
  "name": "pi-web",
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": "^24"
  },
  "description": "Web UI for the pi coding agent",
  "repository": {
    "type": "git",
    "url": "https://github.com/ravshansbox/pi-web.git"
  },
  "bin": {
    "pi-web": "bin/pi-web.js"
  },
  "files": [
    "bin",
    "dist",
    "build/server",
    "README.md"
  ],
  "scripts": {
    "dev": "npm run dev:pi",
    "dev:pi": "concurrently \"vite build --watch\" \"tsc -w -p tsconfig.server.json\" \"node --watch build/server/server.js --agent pi\"",
    "dev:omp": "concurrently \"vite build --watch\" \"tsc -w -p tsconfig.server.json\" \"node --watch build/server/server.js --agent omp\"",
    "build": "vite build && tsc -p tsconfig.server.json",
    "build:bin": "node scripts/generate-bin.mjs",
    "preview": "vite preview",
    "start": "node build/server/server.js",
    "prepack": "npm run build && npm run build:bin"
  },
  "dependencies": {
    "ws": "^8.19.0"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.2.1",
    "@types/node": "^25.3.2",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@types/ws": "^8.18.1",
    "@vitejs/plugin-react": "^5.1.4",
    "concurrently": "^9.2.1",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-router-dom": "^7.13.1",
    "tailwindcss": "^4.2.1",
    "typescript": "^5.9.3",
    "vite": "^7.3.1"
  }
}
